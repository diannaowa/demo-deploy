{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "webui",
    "namespace": "${NAMESPACE}",
    "labels": {
      "name": "webui"
    }
  },
  "spec": {
    "type": "NodePort",
    "ports": [
      {
        "targetPort": 80,
        "port": 8000,
        "nodePort": "${SVC_PORT}"
      }
    ],
    "selector": {
      "tier": "webui"
    }
  }
}
